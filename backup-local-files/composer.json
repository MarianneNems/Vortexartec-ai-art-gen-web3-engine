{
    "name": "vortexartec/vortex-ai-engine",
    "description": "VORTEX AI Engine For the ARTS - Advanced AI agent orchestration system with vault integration and blockchain marketplace features",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "version": "2.1.0",
    "require": {
        "php": ">=7.4",
        "aws/aws-sdk-php": "^3.321.0",
        "guzzlehttp/guzzle": "^7.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "VortexAIEngine\\": "includes/",
            "VortexAIEngine\\Shortcodes\\": "includes/shortcodes/",
            "VortexAIEngine\\Admin\\": "includes/admin/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 